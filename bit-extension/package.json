{
    "name": "bit",
    "displayName": "Bit",
    "description": "vs code bit language highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bit",
            "aliases": ["BIT", "bit"],
            "extensions": [".bit"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bit",
            "scopeName": "source.bit",
            "path": "./syntaxes/bit.tmLanguage.json"
        }]
    }
}